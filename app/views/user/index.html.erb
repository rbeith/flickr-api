	<%= form_with method: :get do |form| %>
		Enter 
		<%= form.label :user_id, "Flickr User ID" %>
		<%= form.text_field :user_id %>
		<%= form.submit "Get Photos" %>
	<% end %>
		
	<% if !@photos.nil? %>
		<% @photos.each do |photo| %>
		  <% @url = "https://live.staticflickr.com/#{photo.server}/#{photo.id}_#{photo.secret}.jpg}"%>
			<%p @url %>
			<div>
				<%= image_tag "#@url" %>
			</div>
		<% end %>
	<% end %>